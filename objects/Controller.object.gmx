            <string>var powerBarMaxWidth = 100;

if ( global.sling &amp;&amp; mouse_check_button(mb_left) &amp;&amp; global.throwable.clicked==true) {
    if (mouseAlreadyPressed == false) {
        startX = global.throwable.x;
        startY = global.throwable.y;
        mouseAlreadyPressed = true;
    }
    else if (mouseAlreadyPressed == true) {
        lineLength = point_distance(mouse_x, mouse_y, startX, startY);
        
        if (lineLength &gt; global.lineLengthMaxVelocity){
            lineLength = global.lineLengthMaxVelocity;
        }
       
        draw_set_alpha(0.5);
        
        draw_line_width_color(mouse_x, mouse_y, startX, startY, powerBarMaxWidth * (lineLength/global.lineLengthMaxVelocity), 
            merge_color(c_blue, c_red, lineLength/global.lineLengthMaxVelocity), 
            merge_color(c_blue, c_red, lineLength/global.lineLengthMaxVelocity)
        );
        
        // Draw white arrow
        draw_set_color(c_white);
        draw_arrow( startX, startY, mouse_x, mouse_y, powerBarMaxWidth * (lineLength/global.lineLengthMaxVelocity) * 0.7);
        
        draw_set_alpha(1);
    }
}
else {
    mouseAlreadyPressed = false;
}
